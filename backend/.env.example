# Backend environment variables
# PostgreSQL connection (SQLAlchemy/SQLModel URL)
DATABASE_URL=postgresql+psycopg://postgres:postgres@localhost:5432/llmchecks

# Primary generation model (used to produce outputs)
LLM_PROVIDER=gemini
LLM_MODEL=gemini-1.5-flash

# Judge model (used to evaluate outputs)
JUDGE_PROVIDER=gemini
JUDGE_MODEL=gemini-1.5-flash

# API keys
# For Gemini (Google AI Studio)
GEMINI_API_KEY=replace-with-your-google-ai-studio-key
# Optional for LiteLLM/OpenAI
OPENAI_API_KEY=